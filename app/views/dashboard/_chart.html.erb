<div class="container">
  <div data-controller="dashboard">
    <div data-dashboard-target="table">
      <%= render "dashboard/table"%>
    </div>

    <div data-dashboard-target="circle" class="d-none">
      <%= render "dashboard/circle"%>
    </div>

    <div class="d-flex justify-content-between navbar-menu z-1 pt-5">
      <h6 data-action="click->dashboard#horoscope" data-dashboard-target="table_menu" class="lead"><em>Table</em></h6>
      <h6>|</h6>
      <h6 data-action="click->dashboard#profile" data-dashboard-target="circle_menu"><em>Circle</em></h6>
    </div>
</div>
